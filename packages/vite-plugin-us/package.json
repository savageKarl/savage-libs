{
	"name": "vite-plugin-us",
	"version": "1.0.12",
	"description": "A vite plugin for developing and building scripts that run on browser extensions such as Tampermonkey, Violentmonkey., Greasemonkey and ScriptCat.",
	"miniprogram": "dist",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"buildOptions": {
		"format": [
			"esm",
			"cjs"
		],
		"dts": true,
		"minify": true,
		"target": [
			"esnext"
		],
		"copy": {
			"from": "src/types",
			"to": "dist/types"
		}
	},
	"type": "module",
	"scripts": {},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@types/connect": "^3.4.36"
	},
	"dependencies": {
		"axios": "^1.5.0",
		"connect": "^3.7.0",
		"get-port": "7.0.0",
		"jsdom": "^22.1.0",
		"open": "9.1.0",
		"picox": "workspace:^",
		"rollup": "^3.2.3",
		"savage-utils": "workspace:^",
		"vite": "^4.4.11"
	}
}
